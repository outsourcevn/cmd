    <div class="container">
        <div class="row">
            <article class="col-sm-12 col-xs-6 article wow fadeInUp animated" data-wow-delay="0.2s" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInUp;">
                <div class="wrap-title">
                    <h3 class="title-lg">
                        <a href="#">Video</a>
                    </h3>
                    <a href="#" class="view-all">
                        <i class="icon icon-view-all"></i>
